<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Section xmlns:xi="http://www.w3.org/2001/XInclude" id="3c477453-a223-399f-b9c2-6dc996ded63f" title="agent-label-unassign" xmlns="urn:com.io7m.structural:8:0">
  <Subsection title="Name">
    <Paragraph>
      <Term type="command">agent-label-unassign</Term>
       - Unassign labels from an agent.
    </Paragraph>
  </Subsection>
  <Subsection title="Description">
    <Paragraph>
      The 
      <Term type="command">agent-label-unassign</Term>
       command unassigns labels from agents.
    </Paragraph>
  </Subsection>
  <xi:include href="scmd-agent-label-unassign-parameters.xml"/>
  <Subsection title="Examples">
    <FormalItem title="Example" type="example">
      <Verbatim><![CDATA[
[northpike]$ agent-label-unassign --agent b6da687b-8de2-41c4-8dca-1784358a029d --label os.linux
]]></Verbatim>
    </FormalItem>
  </Subsection>
</Section>

