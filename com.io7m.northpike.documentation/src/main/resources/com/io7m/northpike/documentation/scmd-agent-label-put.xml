  <Subsection title="Name">
    <Paragraph>
      <Term type="command">agent-label-put</Term>
       - Create/update an agent label.
    </Paragraph>
  </Subsection>
  <Subsection title="Description">
    <Paragraph>
      The 
      <Term type="command">agent-label-put</Term>
       command creates or updates a label.
    </Paragraph>
  </Subsection>
  <xi:include href="scmd-agent-label-put-parameters.xml"/>
  <Subsection title="Examples">
    <FormalItem title="Example" type="example">
      <Verbatim><![CDATA[
[northpike]$ agent-label-put --name os.linux --description 'The agent is running Linux.'
]]></Verbatim>
    </FormalItem>
  </Subsection>
