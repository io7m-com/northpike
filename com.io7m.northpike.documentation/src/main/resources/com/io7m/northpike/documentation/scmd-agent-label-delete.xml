  <Subsection title="Name">
    <Paragraph>
      <Term type="command">agent-label-delete</Term>
       - Delete agent labels.
    </Paragraph>
  </Subsection>
  <Subsection title="Description">
    <Paragraph>
      The 
      <Term type="command">agent-label-delete</Term>
       command deletes labels from agents.
    </Paragraph>
  </Subsection>
  <xi:include href="scmd-agent-label-delete-parameters.xml"/>
  <Subsection title="Examples">
    <FormalItem title="Example" type="example">
      <Verbatim><![CDATA[
[northpike]$ agent-label-delete --name os.linux --name os.windows
]]></Verbatim>
    </FormalItem>
  </Subsection>
</Section>

